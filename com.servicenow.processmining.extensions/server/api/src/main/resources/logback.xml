<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<Pattern>%d{ISO8601} %-5p [%t] - %m%n</Pattern>
		</encoder>
	</appender>

	<logger name="com.servicenow.processmining.extensions.server.jobs" level="INFO" />
	<logger name="com.servicenow.processmining.extensions.server.api.controller" level="INFO" />

	<logger name="com.servicenow.processmining.extensions.simulation.core" level="INFO" />
	<logger name="com.servicenow.processmining.extensions.simulation.workflow" level="INFO" />

	<logger name="com.servicenow.processmining.extensions.pm.dao" level="INFO" />

	<logger name="com.servicenow.processmining.extensions.sc.dao" level="INFO" />

	<logger name="com.servicenow.processmining.extensions.sn.core" level="INFO" />

	<root level="OFF">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>
